<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #A9A9A9;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/style.css" />
    </stylesheets>
    <children>
      <TableView fx:id="carTable" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="570.0" style="-fx-background-color: white#60affe;">
        <columns>
          <TableColumn fx:id="colRegistrationCar" prefWidth="41.0" style="-fx-background-color: #D3D3D3;" text="ID" />
          <TableColumn fx:id="colManufacturerCar" prefWidth="109.0" style="-fx-background-color: #D3D3D3;" text="Manufacturer" />
            <TableColumn fx:id="colModelCar" prefWidth="68.0" style="-fx-background-color: #D3D3D3;" text="Model" />
            <TableColumn fx:id="colDescriptionCar" prefWidth="91.0" style="-fx-background-color: #D3D3D3;" text="Description" />
            <TableColumn fx:id="colFuelCar" prefWidth="47.0" style="-fx-background-color: #D3D3D3;" text="Fuel" />
            <TableColumn fx:id="colPriceCar" prefWidth="120.0" style="-fx-background-color: #D3D3D3;" text="Price(per day)" />
            <TableColumn fx:id="colCustomer" prefWidth="100.0" style="-fx-background-color: #D3D3D3;" text="Customer" />
        </columns>
      </TableView>
      <GridPane layoutX="14.0" layoutY="296.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnBack" alignment="CENTER" mnemonicParsing="false" onAction="#previousWindow" prefHeight="31.0" prefWidth="96.0" style="-fx-background-color: #696969;" text="Back" textAlignment="CENTER" textFill="WHITE" GridPane.rowIndex="2">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </GridPane>
      <GridPane layoutX="386.0" layoutY="296.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnExit" alignment="CENTER" mnemonicParsing="false" onAction="#exitWindow" prefHeight="31.0" prefWidth="96.0" style="-fx-background-color: #696969;" text="Exit" textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </GridPane>
      <GridPane layoutX="200.0" layoutY="231.0" prefHeight="118.0" prefWidth="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="0.0" prefWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="addCarBtn" mnemonicParsing="false" onAction="#addCar" prefHeight="25.0" prefWidth="203.0" style="-fx-background-color: #696969;" text="Add" textFill="WHITE">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="editCarBtn" mnemonicParsing="false" onAction="#editCar" prefHeight="25.0" prefWidth="203.0" style="-fx-background-color: #696969;" text="Edit" textFill="WHITE" GridPane.rowIndex="1">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="deleteCarBtn" mnemonicParsing="false" onAction="#deleteCar" prefHeight="22.0" prefWidth="203.0" style="-fx-background-color: #696969;" text="Delete" textFill="WHITE" GridPane.rowIndex="2">
               <font>
                  <Font size="15.0" />
               </font></Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
